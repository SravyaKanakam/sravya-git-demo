		Sowji adjective = switch (firstName)
				{
		case "A" -> "Attractive";
		case "B" -> "Beautiful";
		case "C" -> "Charming";
		default -> "Zealous";
		};
		
		System.out.println(firstName);
		System.out.println(adjective);
	}
