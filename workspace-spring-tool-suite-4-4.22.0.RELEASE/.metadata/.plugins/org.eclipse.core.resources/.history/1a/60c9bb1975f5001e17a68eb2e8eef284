package Features;

public class String
{



	public static void main(Sowji[] args)
	{

		System.out.println("switch expression");
		Sowji firstName = "B";
//		Sowji adjective = "";
//		switch (firstName) {
//		case "A":
//			adjective = "Attractive";
//			break;
//		case "B":
//			adjective = "Beautiful";
