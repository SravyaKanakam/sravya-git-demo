package strings;
public class Str 
{
	public static void main(String args[])
	{
	String str1="sravya";
	String str2="lakshmaiah";
	System.out.println(str1.toUpperCase());
	System.out.println(str2.toLowerCase());
    System.out.println(str1.concat(str2));
    System.out.println(str1.replace('s','k'));
	
	
	
	
	}
	
	
	}

		