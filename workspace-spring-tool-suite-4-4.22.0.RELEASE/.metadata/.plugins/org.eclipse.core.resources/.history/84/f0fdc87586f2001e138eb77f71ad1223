package Sravya;
import java.util.*;
public class B extends A//inheritance
{
	int m1(int a)//method overriding
	{
		super.m1(20);
		System.out.println(a);
		return a;
	}
	int m2(int x)
	{
		System.out.println(x);
		return x;
	}
	public static void main(String args[])
	{
		
		B x=new B();
		System.out.println(x.m2(88));
		System.out.println(x.m1(10));
	}
}

